<% if current_user %>
  <%= button_to "Log Out", session_path(current_user.id), method: :delete %>
<% else %>
  <%= button_to "Register", "/register", {method: "get"}  %>
  <%= button_to "Login", "/login", {method: "get"}  %>
<% end %>
<br>

<div id="existing-users">
  <h3> Existing Users </h3>
  <% @users.each do |user| %>
    <li><%= link_to "#{user.email}", user_path(user.id) %></li>
  <% end %>
</div>